<div class="container">
  
  <h1>Top Twenty Dance Tracks</h1>
   
  <div class="col">
    
    <div class="row">
        {{#each shows}}
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <a id="song-name" href="{{this.songLink}}" target="_blank"><h5 class="card-title">{{this.title}}</h5></a>
            <a id="artist-name" href="{{this.artistLink}}" target="_blank"><h6 class="card-subtitle mb-2 text-muted">{{this.artist}}</h6></a>
            <a id="label-name" href="{{this.labelLink}}" target="_blank"><p class="card-text">{{this.label}}</p></a>
            <a href="#" class="card-link like-track">Like Track</a>
            <a href="#" class="card-link add-note">Add Note</a>
          </div>
        </div>
        {{/each}}
    </div>

</div>